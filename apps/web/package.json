{
  "name": "web",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite dev --host --port 3000",
    "build": "tsc --noEmit && vite build",
    "preview": "vite preview --host --port 3000",
    "lint": "eslint .",
    "check-types": "tsc --noEmit",
    "i18n:extract": "formatjs extract \"src/**/*.{ts,tsx}\" --ignore \"src/**/*.d.ts\" --out-file src/i18n/locales/en.json --format ./scripts/formatjs-formatter.mjs --id-interpolation-pattern \"[sha1:contenthash:base64:6]\"",
    "i18n:sync": "node ./scripts/i18n-sync.mjs"
  },
  "dependencies": {
    "marked": "^16.4.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-intl": "^7.1.14",
    "react-router-dom": "^6.28.0"
  },
  "devDependencies": {
    "@formatjs/cli": "^6.7.4",
    "@repo/eslint-config": "*",
    "@repo/typescript-config": "*",
    "@types/node": "^22.15.3",
    "@types/react": "19.1.0",
    "@types/react-dom": "19.1.1",
    "@vitejs/plugin-react": "^5.1.1",
    "babel-plugin-formatjs": "^10.5.41",
    "eslint": "^9.34.0",
    "typescript": "5.9.2",
    "vite": "^7.3.0",
    "vite-plugin-svgr": "^4.5.0"
  }
}
