{
  "name": "desktop",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "node scripts/select-platform-dev.mjs",
    "dev:mac": "cross-env TAURI_PLATFORM=macos npm run dev:tauri",
    "dev:windows": "cross-env TAURI_PLATFORM=windows npm run dev:tauri",
    "dev:windows:gpu": "cross-env TAURI_PLATFORM=windows tauri dev --config src-tauri/tauri.local.conf.json -- --features windows-gpu",
    "dev:linux": "cross-env TAURI_PLATFORM=linux npm run dev:tauri",
    "dev:linux:gpu": "cross-env TAURI_PLATFORM=linux npm run dev:tauri:gpu",
    "dev:tauri": "tauri dev --config src-tauri/tauri.local.conf.json",
    "dev:tauri:gpu": "tauri dev --config src-tauri/tauri.local.conf.json -- --no-default-features --features linux-gpu",
    "dev:vite": "node scripts/run-vite-with-flavor.mjs dev",
    "build": "tsc && node scripts/run-vite-with-flavor.mjs build",
    "preview": "node scripts/run-vite-with-flavor.mjs preview",
    "tauri": "tauri",
    "test:webdriver": "wdio run webdriver/wdio.conf.js",
    "i18n:extract": "node ./scripts/i18n-extract-with-prune.mjs",
    "i18n:sync": "node ./scripts/i18n-sync.mjs",
    "i18n": "npm run i18n:extract && npm run i18n:sync",
    "lint:fix": "prettier --write src && oxlint --fix src",
    "lint": "prettier --check src && oxlint src"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@fontsource/roboto": "^5.2.5",
    "@mui/icons-material": "^7.1.0",
    "@mui/lab": "^7.0.0-beta.14",
    "@mui/material": "^7.1.0",
    "@repo/functions": "*",
    "@repo/pricing": "*",
    "@repo/types": "*",
    "@repo/utilities": "*",
    "@repo/voice-ai": "*",
    "@stripe/react-stripe-js": "^3.7.0",
    "@stripe/stripe-js": "^7.4.0",
    "@tauri-apps/api": "^2.8.0",
    "@tauri-apps/plugin-autostart": "^2.5.1",
    "@tauri-apps/plugin-http": "^2.5.4",
    "@tauri-apps/plugin-opener": "^2",
    "@tauri-apps/plugin-os": "^2.3.1",
    "@tauri-apps/plugin-process": "^2.3.0",
    "@tauri-apps/plugin-sql": "^2",
    "@tauri-apps/plugin-updater": "^2.9.0",
    "chroma-js": "^3.2.0",
    "dayjs": "^1.11.13",
    "firebase": "^11.10.0",
    "immer": "^10.1.1",
    "lodash-es": "^4.17.21",
    "react": "^19.1.0",
    "react-confetti": "^6.4.0",
    "react-dom": "^19.1.0",
    "react-error-boundary": "^6.0.0",
    "react-intl": "^7.1.14",
    "react-markdown": "^10.1.0",
    "react-router-dom": "^6.29.0",
    "react-transition-group": "^4.4.5",
    "react-virtuoso": "^4.12.8",
    "rxjs": "^7.8.2",
    "vite-plugin-svgr": "^4.5.0",
    "zod": "^3.25.76",
    "zod-to-json-schema": "^3.24.6",
    "zustand": "^4.5.2"
  },
  "devDependencies": {
    "@formatjs/cli": "^6.7.4",
    "@tauri-apps/cli": "^2",
    "@types/chroma-js": "^3.1.2",
    "@types/lodash-es": "^4.17.12",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@vitejs/plugin-react": "^4.6.0",
    "@wdio/cli": "^9.20.0",
    "@wdio/globals": "^9.17.0",
    "@wdio/local-runner": "^9.20.0",
    "@wdio/mocha-framework": "^9.20.0",
    "@wdio/spec-reporter": "^9.20.0",
    "babel-plugin-formatjs": "^10.5.41",
    "chromedriver": "^141.0.0",
    "cross-env": "^7.0.3",
    "oxlint": "^1.32.0",
    "prettier": "^3.7.4",
    "ts-node": "^10.9.2",
    "typescript": "~5.8.3",
    "vite": "^7.0.4"
  }
}
